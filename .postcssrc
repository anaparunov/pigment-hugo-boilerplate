{
  "parser": "sugarss",
  "plugins": {
    "postcss-easy-import": {
      "extensions": [
        ".sss",
        ".css"
      ]
    },
    "postcss-preset-env": {
      stage: 0,
      features: {
        'nesting-rules': false,
      },
      browsers: [
        '>0.1%',
        'firefox esr',
      ],
    },
    "postcss-custom-media": {},
    "postcss-color-function": {},
    "postcss-nesting":{},
    "postcss-current-selector":{},
    "rucksack-css": {},
    "postcss-mixins": {},
  }
}
